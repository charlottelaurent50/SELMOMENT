<body class="home">
  <!-- Fixed navbar -->
  <div class="navbar navbar-inverse navbar-fixed-top headroom" >
    <div class="container">
      <div class="navbar-header">
        <!-- Button for smallest screens -->
        <a class="navbar-brand" href="{{path('route_accueil')}}"><img class="logo_img" src="{{asset('images/logo_white.png')}}" alt="logo"></a>
        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse"><span class="icon-bar"></span> <span class="icon-bar"></span> <span class="icon-bar"></span> </button>
      </div>
      <div class="navbar-collapse collapse">
        <ul class="nav navbar-nav pull-right">
          <li {% block accueil %} {% endblock %}><a href="{{path('route_accueil')}}">Accueil</a></li>
          <li {% block inscription %} {% endblock %}><a href="{{path('inscription')}}">Devenir membre</a></li>
          <li {% block vieselmoment %} {% endblock %}><a href="about.html">La vie de SEL Moment</a></li>
            <li class="dropdown">
              <a href="" class="dropdown-toggle" data-toggle="dropdown">Annonces <b class="caret"></b></a></a>
              <ul class="dropdown-menu">
              {% if is_granted('IS_AUTHENTICATED_FULLY') %}
                <li {% block annonce %} {% endblock %}> <a href="{{path('annonceLister')}}">Toutes les annonces</a></li>
                <li {% block offre %} {% endblock %}> <a href="#">Offre</a></li>
                <li {% block demande %} {% endblock %}><a href="#">Demande</a></li>
              {% else %}
                <li><a href="#">Réserver aux adhérents</a></li>
              {% endif %}
              </ul>
            </li>
              {% if is_granted('IS_AUTHENTICATED_FULLY') %} 
                <li><a class="btn" href="{{ path('route_accueil') }}">Profil</a></li>
                <li><a class="btn" href="{{ path('app_logout') }}">Deconnexion</a></li>
              {% else %}
                <li><a class="btn" href="{{ path('app_login') }}">Connexion</a></li>
              {% endif %}
        </ul>
      </div><!--/.nav-collapse -->
    </div>
  </div> 
  <!-- /.navbar -->

  
